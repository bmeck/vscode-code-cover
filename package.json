{
  "name": "vscode-code-cover",
  "description": "Hightlights lines with code coverage",
  "version": "1.0.0",
  "license": "MIT",
  "publisher": "bradleymeck",
  "displayName": "Code Cover",
  "icon": "images/code-cover.png",
  "scripts": {
    "build": "make"
  },
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/index.js",
  "contributes": {
    "commands": [
      {
        "command": "ShowCoverage",
        "title": "Highlight code coverage"
      },
      {
        "command": "HideCoverage",
        "title": "Hide code coverage"
      }
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "vscode": "0.10.x"
  },
  "dependencies": {
    "babel-runtime": "^6.2.0",
    "generator-runner": "^2.2.0",
    "glob": "^6.0.1",
    "lcov-parse": "0.0.10",
    "minimatch": "^3.0.0",
    "source-map": "^0.5.3",
    "source-map-url": "^0.4.0"
  }
}
